{
  "compilerOptions": {
    "types": ["@cloudflare/workers-types/experimental"],
    "lib": ["es2021"],
    "target": "es2021",
    "jsx": "react",
    "module": "es2022",
    "moduleResolution": "Bundler" ,
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
